#include <iostream>
#include <string>
using namespace std;

int main() {
  string s;
  cin >> s; // read the input string
  int n = s.length(); // get the length of the string
  int count = 1; // initialize the count of consecutive players
  bool dangerous = false; // initialize the flag for dangerous situation
  for (int i = 1; i < n; i++) { // loop through the string from the second character
	if (s[i] == s[i-1]) { // if the current character is the same as the previous one
	  count++; // increment the count
	  if (count >= 7) { // if the count is at least 7
		dangerous = true; // set the flag to true
		break; // break the loop
	  }
	} else { // if the current character is different from the previous one
	  count = 1; // reset the count to 1
	}
  }
  if (dangerous) { // if the flag is true
	cout << "YES" << endl; // print "YES"
  } else { // if the flag is false
	cout << "NO" << endl; // print "NO"
  }
  return 0;
}
