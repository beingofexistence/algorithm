#include <iostream>
#include <string>
using namespace std;

int main() {
  string s;
  cin >> s; 
  int n = s.length(); 
  int count = 1; 
  bool dangerous = false; 
  for (int i = 1; i < n; i++) { 
	if (s[i] == s[i-1]) { 
	  count++; // increment the count
	  if (count >= 7) { 
		dangerous = true;
		break; // break the loop
	  }
	} else { 
	  count = 1; 
	}
  }
  if (dangerous) { // if the flag is true
	cout << "YES" << endl; // print "YES"
  } else { // if the flag is false
	cout << "NO" << endl; // print "NO"
  }
  return 0;
}
